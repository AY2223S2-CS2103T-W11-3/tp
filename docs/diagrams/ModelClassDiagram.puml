@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Class "<<interface>>\nReadOnlyMasterDeck" as ReadOnlyMasterDeck
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class MasterDeck
Class ModelManager
Class UserPrefs

Class UniqueCardList
Class Card
Class Address
Class Email
Class Name
Class Phone
Class Tag

}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

MasterDeck .up.|> ReadOnlyMasterDeck

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyMasterDeck
ModelManager -left-> "1" MasterDeck
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

MasterDeck *--> "1" UniqueCardList
UniqueCardList --> "~* all" Card
Card *--> Name
Card *--> Phone
Card *--> Email
Card *--> Address
Card *--> "*" Tag

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager -->"~* filtered" Card
@enduml
